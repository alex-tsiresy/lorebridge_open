# YouTube Transcript Service Configuration
# Copy this file to .env and fill in your credentials

# Webshare Proxy Configuration (Recommended)
# Get these from: https://www.webshare.io/proxy-settings
WEBSHARE_USERNAME=your_username_here
WEBSHARE_PASSWORD=your_password_here

# Deepgram Configuration (Required for audio transcription)
# Get your API key from: https://logger.deepgram.com/
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# Gemini Configuration (Required for video summarization)
# Get your API key from: https://ai.google.dev/
GEMINI_API_KEY=your_gemini_api_key_here

# Security Configuration
# Comma-separated list of API keys for service authentication
# Generate secure keys using: python -c "import secrets; print(secrets.token_urlsafe(32))"
API_KEYS=your_api_key_1,your_api_key_2,your_api_key_3

# Service Configuration
PORT=5001 